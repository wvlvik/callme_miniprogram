/* common */


/* header */
.page-name {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 150rpx;
  background-color: #f2f2f2;
  box-sizing: border-box;
  border-bottom: 1px #E8E8E8 solid;
}

.page-name text {
  font-size: 24px;
  font-weight: bold;
}



.page-content {
  width: 650rpx;
  margin: auto;
  padding-bottom: 120px;
}






/* navigator */
.navigator-hover, .navigator-active {
  background-color: rgba(0,0,0,0);
  color: #008E1C;
}